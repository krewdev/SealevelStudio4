# ============================================
# Sealevel Studio - Environment Variables Template
# ============================================
# Copy this file to .env.local and fill in your values
# DO NOT commit .env.local to git!

# ============================================
# Application URL
# ============================================
# Your production domain URL (used for OAuth callbacks, webhooks, etc.)
# For local development, leave as localhost
# For production, set to your domain: https://sealevelstudio.xyz
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Base URL (alternative, used in some API routes)
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# ============================================
# Solana RPC Endpoints
# ============================================
# These are used for connecting to Solana networks
# You can use public RPCs or your own Helius/QuickNode endpoints

# Mainnet RPC endpoint (with API key if using Helius/QuickNode)
NEXT_PUBLIC_SOLANA_RPC_MAINNET=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY

# Devnet RPC endpoint (with API key if using Helius/QuickNode)
NEXT_PUBLIC_SOLANA_RPC_DEVNET=https://devnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY

# Default network (mainnet or devnet)
NEXT_PUBLIC_SOLANA_NETWORK=mainnet

# Alternative RPC URL (used as fallback)
NEXT_PUBLIC_RPC_URL=

# ============================================
# API Keys (Server-side only - never expose to client)
# ============================================
# These are used in Next.js API routes and are never sent to the browser

# Helius API Key (for enhanced RPC features and DAS API)
HELIUS_API_KEY=your_helius_api_key_here

# Birdeye API Key (for token price data and market information)
BIRDEYE_API_KEY=your_birdeye_api_key_here

# Dune Analytics API Key (for on-chain analytics queries)
DUNE_API_KEY=your_dune_api_key_here

# Solscan API Key (for transaction and account data)
SOLSCAN_API_KEY=your_solscan_api_key_here

# Jupiter API Key (for swap quotes and transactions - higher rate limits)
# Also enables Jupiter Ultra API (https://api.jup.ag/ultra) for simplified swap execution
JUPITER_API_KEY=your_jupiter_api_key_here

# ============================================
# Client-side API Keys (exposed to browser)
# ============================================
# These are prefixed with NEXT_PUBLIC_ and will be available in the browser
# Only use for APIs that require client-side access

# Helius API Key (for client-side pool fetching)
NEXT_PUBLIC_HELIUS_API_KEY=your_helius_api_key_here

# Helius Fast RPC Endpoint (optional, for faster transaction processing)
# Example: https://sender.helius-rpc.com/fast
# If set, this will be used instead of standard Helius RPC
NEXT_PUBLIC_HELIUS_FAST_RPC=

# Birdeye API Key (for client-side price fetching)
NEXT_PUBLIC_BIRDEYE_API_KEY=your_birdeye_api_key_here

# QuickNode API Key (for WebSocket streaming and RPC access)
# Get your API key from: https://www.quicknode.com/
QUICKNODE_API_KEY=your_quicknode_api_key_here

# QuickNode Endpoint (optional, if you want to construct WebSocket URL from API key)
# Format: your-endpoint.solana-mainnet.quiknode.pro
QUICKNODE_ENDPOINT=your-endpoint.solana-mainnet.quiknode.pro

# ============================================
# WebSocket URLs (Optional - for real-time updates)
# ============================================

# QuickNode WebSocket URL (for real-time account updates via Solana WebSocket streaming)
# Option 1: Provide full URL directly
# Format: wss://your-endpoint.solana-mainnet.quiknode.pro/your-api-key/
# NEXT_PUBLIC_QUICKNODE_WS_URL=wss://your-endpoint.solana-mainnet.quiknode.pro/your-api-key/
# Option 2: Will be auto-constructed from QUICKNODE_ENDPOINT + QUICKNODE_API_KEY if not provided
NEXT_PUBLIC_QUICKNODE_WS_URL=

# QuickNode Stream Filter (fallback RPC endpoint when WebSocket stream has gaps)
# Recommended: Use your Helius RPC endpoint for best performance
# Format: https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY
# Alternative: https://api.mainnet-beta.solana.com (public, rate-limited)
QUICKNODE_STREAM_FILTER=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY

# Helius WebSocket URL (fallback for real-time pool updates)
NEXT_PUBLIC_HELIUS_WS_URL=wss://atlas-mainnet.helius-rpc.com/?api-key=YOUR_KEY

