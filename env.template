# ============================================
# SEAL Token Configuration
# ============================================
# SEAL is the native token for Sealevel Studio

# SEAL Treasury Seed (for airdrop and treasury management)
# This should be a secure seed phrase or keypair seed
SEAL_TREASURY_SEED=your_treasury_seed_phrase_here

# ============================================
# Platform Fee Configuration
# ============================================
# Platform fees collected on each transaction

# Platform Fee Address (where transaction fees are sent)
# This address receives the 0.0002 SOL fee per transaction
NEXT_PUBLIC_PLATFORM_FEE_ADDRESS=your_platform_fee_wallet_address_here

# Alternative: Treasury Address (used as fallback if platform fee address not set)
# This can be the same as the platform fee address
NEXT_PUBLIC_TREASURY_ADDRESS=your_treasury_wallet_address_here

# ============================================
# AI Configuration
# ============================================
# You can use LOCAL AI only, EXTERNAL APIs only, or BOTH (with local as primary)

# ============================================
# Local AI Model (Recommended - No API Keys Needed!)
# ============================================
# Use a downloaded/local AI model - FREE and PRIVATE
# Supports Ollama, LM Studio, and OpenAI-compatible endpoints

# Enable local AI model (set to true to use downloaded model)
# If enabled, this will be used as the PRIMARY AI provider
LOCAL_AI_ENABLED=true

# Local AI endpoint URL
# Ollama default: http://localhost:11434
# LM Studio default: http://localhost:1234/v1
LOCAL_AI_ENDPOINT=http://localhost:1234/v1

# Local AI model name (if using Ollama)
LOCAL_AI_MODEL=llama2

# Local AI API type: 'ollama' | 'lmstudio' | 'openai-compatible' | 'custom'
LOCAL_AI_TYPE=lmstudio

# Local AI timeout in milliseconds (default: 30000)
LOCAL_AI_TIMEOUT=30000

# ============================================
# External AI API Keys (OPTIONAL - Fallback Only)
# ============================================
# These are OPTIONAL and only used as fallback if local AI fails
# You can leave these empty if using local AI only

# OpenAI API Key (OPTIONAL - only needed for DALL-E image generation or fallback)
# For text generation, local AI is sufficient
OPENAI_API_KEY=

# Anthropic Claude API Key (OPTIONAL - fallback only)
ANTHROPIC_API_KEY=

# Google Gemini API Key (OPTIONAL - fallback only)
GEMINI_API_KEY=

# DeepSeek API (OPTIONAL - for self-hosted DeepSeek models)
DEEPSEEK_API_URL=
DEEPSEEK_API_KEY=

# Model name to use
# Ollama examples: llama2, codellama, mistral, phi
# LM Studio: Use the model name from LM Studio
LOCAL_AI_MODEL=llama2

# API type: ollama, lmstudio, openai-compatible, custom
LOCAL_AI_TYPE=ollama

# ============================================
# LM Studio Configuration (for Bleeding Edge UI)
# ============================================
# LM Studio endpoint for Generative UI and Command Bar
# Default: http://localhost:1234/v1 (LM Studio's OpenAI-compatible API)
NEXT_PUBLIC_LM_STUDIO_ENDPOINT=http://localhost:1234/v1

# ============================================
# Pump.fun Configuration
# ============================================
# Pump.fun Program ID (for QuickNode stream filter)
# This is the program ID that pump.fun uses for token launches
# Update with actual pump.fun program ID
PUMPFUN_PROGRAM_ID=6EF8rrecthR5Dkzon8Nwu78hRvfCKubJ14M5uBEwF6P

# Pump.fun WebSocket URL (fallback if not using QuickNode)
# Note: Only used if QuickNode is not configured
# Format: wss://api.pump.fun/stream or similar
NEXT_PUBLIC_PUMPFUN_WS_URL=wss://pumpfun-api.com/stream

# Use as primary/core model (higher weight in consensus)
LOCAL_AI_PRIMARY=true

# Weight in consensus (higher = more influence, default 1.5 for core model)
LOCAL_AI_WEIGHT=1.5

# Request timeout in milliseconds
LOCAL_AI_TIMEOUT=30000

# ============================================
# Docker Configuration
# ============================================
# Enable Docker for local AI (uses docker-compose)

# Use Docker for local AI services
DOCKER_AI_ENABLED=false

# Docker container name for Ollama
DOCKER_AI_CONTAINER=sealevel-ollama

# ============================================
# MCP (Model Context Protocol) Configuration
# ============================================
# MCP provides standardized tool access for AI models

# Enable MCP server
MCP_ENABLED=false

# MCP server endpoint URL
MCP_SERVER_URL=http://localhost:8000

# MCP server API key (generate with: openssl rand -hex 32)
MCP_API_KEY=your_mcp_api_key_here

# MCP log level (debug, info, warn, error)
MCP_LOG_LEVEL=info

# ============================================
# Redis Configuration (Optional)
# ============================================
# Redis is optional - the app works without it using in-memory caching
# Enable Redis for distributed caching, session storage, or production deployments

# Enable Redis connection
REDIS_ENABLED=false

# Redis connection URL (see Database-Stored Variables section below)
# Format: ${{ Redis.REDIS_URL }} or redis://localhost:6379
# Default for local development: redis://localhost:6379
REDIS_URL=redis://localhost:6379

# Redis password (if required)
# REDIS_PASSWORD=your_redis_password_here

# Redis database number (0-15, default: 0)
REDIS_DB=0

# ============================================
# Database-Stored Variables
# ============================================
# These variables should be stored in a database (e.g., Railway, Vercel, etc.)
# Use variable substitution syntax: ${{ Redis.REDIS_URL }}
# 
# Variables that need database storage:
# - Redis connection for caching, sessions, and user data
# - Database connection strings (PostgreSQL, MongoDB, etc.)
# - Encryption keys for keypair storage
# - Session secrets and tokens
# - User authentication data
# - Usage tracking and machine use counts
# - API rate limiting data
# - Webhook secrets and OAuth tokens

# Redis URL (for caching, session storage, user data, keypairs, emails, machine use count)
# Production: Use ${{ Redis.REDIS_URL }} (Railway/Vercel variable substitution)
# Development: Use redis://localhost:6379
# Format: ${{ Redis.REDIS_URL }} or redis://localhost:6379
REDIS_URL=${{ Redis.REDIS_URL }}

# Database Connection URL (PostgreSQL, MongoDB, etc.)
# For storing user data, keypairs, emails, usage tracking
# Format: ${{ Postgres.DATABASE_URL }} or postgresql://user:pass@host:port/db
DATABASE_URL=${{ Postgres.DATABASE_URL }}

# Database connection pool settings
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10

# Encryption key for keypair storage (generate with: openssl rand -hex 32)
# This should be stored securely in database/vault
KEYPAIR_ENCRYPTION_KEY=${{ Variables.KEYPAIR_ENCRYPTION_KEY }}

# Session secret for user sessions (generate with: openssl rand -hex 32)
SESSION_SECRET=${{ Variables.SESSION_SECRET }}

# JWT secret for authentication tokens (generate with: openssl rand -hex 32)
JWT_SECRET=${{ Variables.JWT_SECRET }}

# OAuth state encryption key (generate with: openssl rand -hex 32)
OAUTH_STATE_SECRET=${{ Variables.OAUTH_STATE_SECRET }}

# Webhook signing secret (for verifying webhook requests)
WEBHOOK_SECRET=${{ Variables.WEBHOOK_SECRET }}

# API rate limiting storage (Redis key prefix)
RATE_LIMIT_PREFIX=ratelimit:

# User data storage keys (Redis key prefixes)
USER_DATA_PREFIX=user:
KEYPAIR_STORAGE_PREFIX=keypair:
EMAIL_STORAGE_PREFIX=email:
USAGE_TRACKING_PREFIX=usage:
MACHINE_USE_PREFIX=machine:

# Additional database-stored variables:
# - API key encryption key (for encrypting stored API keys)
API_KEY_ENCRYPTION_KEY=${{ Variables.API_KEY_ENCRYPTION_KEY }}

# - Backup encryption key (for database backups)
BACKUP_ENCRYPTION_KEY=${{ Variables.BACKUP_ENCRYPTION_KEY }}

# - Audit log storage (for security audit trails)
AUDIT_LOG_STORAGE=redis

# - Analytics data storage (for user analytics)
ANALYTICS_STORAGE=redis

# - Transaction history storage (for user transaction logs)
TRANSACTION_HISTORY_STORAGE=redis

# - Wallet recovery key (for wallet recovery mechanisms)
WALLET_RECOVERY_KEY=${{ Variables.WALLET_RECOVERY_KEY }}

# - Two-factor authentication secret (for 2FA setup)
TOTP_SECRET=${{ Variables.TOTP_SECRET }}

# - Email verification token storage (TTL in seconds)
EMAIL_VERIFICATION_TTL=86400

# - Password reset token storage (TTL in seconds)
PASSWORD_RESET_TTL=3600

# - Session storage TTL (in seconds, default: 7 days)
SESSION_TTL=604800

# - Keypair storage encryption algorithm
KEYPAIR_ENCRYPTION_ALGORITHM=aes-256-gcm

# ============================================
# GPU Configuration
# ============================================
# GPU acceleration settings for local AI

# Enable GPU acceleration
GPU_ENABLED=true

# GPU type: auto (detect), nvidia, apple-silicon, intel, cpu
GPU_TYPE=auto

# GPU memory limit in MB (optional, leave empty for unlimited)
# GPU_MEMORY_LIMIT=8192

# ============================================
# Remote AI Server Configuration
# ============================================
# For connecting to a remote Mac server or other remote AI endpoint

# Enable remote AI server
REMOTE_AI_ENABLED=false

# Remote AI server endoint URL
# Example: http://192.168.1.100:11434 (local network)
# Example: https://your-domain.com:11434 (external with SSL)
REMOTE_AI_ENDPOINT=http://your-mac-server.local:11434

# Use SSL/TLS for remote connection
REMOTE_AI_SSL=false
